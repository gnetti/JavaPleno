	<queryString>
		<![CDATA[SELECT c.id AS IdColaborador, c.nome AS NomeColaborador,
SUM(t.valor_dia) AS ValorTotal
 
FROM tb_colaborador AS c 
JOIN tb_trabalho AS t ON c.id = t.colaborador_id
